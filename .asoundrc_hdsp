pcm.!default {
  type asym
  playback.pcm {
    type plug
    slave.pcm {
      type dmix
      ipc_key 1234
      slave {
        pcm {
          type hw
          card "HDSPMx5928ee"
        }
        rate 48000
        channels 16
        buffer_size 2048
        period_size 1024
      }
    }
  }
  capture.pcm {
    type plug
    slave.pcm {
      type dsnoop
      ipc_key 1234
      slave {
        pcm {
          type hw
          card "HDSPMx5928ee"
        }
        format "S32_LE"
        rate 48000
        channels 16
        buffer_size 2048
        period_size 1024
      }
    }
  }
}
